package chap99_homework.homework1019.employee;

public abstract class Employee {
	int eno;
	String name;
	int pay;
	int type;

	public Employee() {
	
	}	
	
	public Employee(int eno, String name, 
			int pay, int type) {
		this.eno = eno;
		this.name = name;
		this.pay = pay;
		this.type = type;
	}
	
	public abstract void eno();
	public abstract void name();
	public abstract void pay();
	public abstract void type();
	public abstract double getMonthPay();
	public abstract void showEmployeeInfo();
	












}