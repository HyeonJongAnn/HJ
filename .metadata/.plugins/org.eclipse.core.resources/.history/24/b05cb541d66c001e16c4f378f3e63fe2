package homework;

public class basic {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//1.
		int num = 10;
		System.out.println(++num);
		System.out.println(++num);
		System.out.println(num--);
		System.out.println(--num);
		System.out.println(num--);
		System.out.println(--num);
		
		//2.
		int a = 10;
		int b = 20;
		
		System.out.println(int a < 11);
		System.out.println(int b > 20);
		
		
		
	}

}
