package chap99_homework.homework1019.employee;

public class EmployeeEx extends Employee {
	 int i = 0;
	@Override
	public void eno() {
		System.out.println("사번 : " + eno);
		// TODO Auto-generated method stub
		
	}

	@Override
	public void name() {
		// TODO Auto-generated method stub
		System.out.println("이름 : " + name);
	}

	@Override
	public void pay() {
		// TODO Auto-generated method stub
		System.out.println("급여: " + pay);
	}

	@Override
	public void type() {
		System.out.println("근무형태: " + type);
		if(i == 1) {
			System.out.println("정규직");
		}else if(i == 2) {
			System.out.println("계약직");
		}else if(i == 3) {
			System.out.println("임시직");
		}else {
			System.out.println("잘못 입력하셨습니다.");
		}
		// TODO Auto-generated method stub
		
	}

	@Override
	public double getMonthPay() {
		// TODO Auto-generated method stub
		System.out.println("월급 : " + pay);
		return 0;
	}

	@Override
	public void showEmployeeInfo() {
		System.out.println("사번 : " + eno);
		System.out.println("이름 : " + name);
		System.out.println("근무형태 : " + type);
		
	}
		// TODO Auto-generated method stub
		
	}
	
	
	
	
	

