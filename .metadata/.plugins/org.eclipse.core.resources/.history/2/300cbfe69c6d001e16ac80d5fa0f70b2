package homework_01;

public class _01_operations {
	
	int a;
	int b;
	
	public void setValue(int a, int b) {
		
	}
	
	public int calculate(){
			
	 private int Add (int a, int b) {
		return a + b;
		
	}
	
	private int Sub (int a, int b) {
		return a - b;
	}
	
	private int Mul (int a, int b) {
		return a * b;
	}
	
	private double Div (int a, int b) {
		if(b == 0) {
			return 0;
		}else {
			return (double)a/b;
		
		}	
	}
}

	
	
}