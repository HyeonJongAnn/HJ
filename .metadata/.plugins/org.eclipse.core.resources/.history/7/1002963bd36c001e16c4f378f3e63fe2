		int b = num%10;
		int cnt=0;
		
		if(num<10) {
			if(b%3 == 0)
				cnt++;
		}else {
			if(a%3 == 0) {
				cnt++;
			}
			if(b%3 == 0 ){
				cnt++;
			}
		}
		
		switch(cnt) {
		case 0:
			System.out.println("박수 없음");
			break;
		case 1:
			System.out.println("박수짝");
			break;
