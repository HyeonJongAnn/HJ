package homework;

public class Middle {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//1.
		for(int i = 1; i <=100; i++) {
			if(i%2 == 0 && i%3 == 0){
				System.out.println(i + "");
				}
		}
		//2.
		int i = 1;
		while(i<=10) {
			if(i%2 == 0)
			System.out.println(i + "");
			i++;
		}
		//3.
		for(int a = 1; a <=1000; a++) {
			if(a%2 == 0 && a%7 == 0){
				System.out.println(a + "");
				}
		
		
		
		
	}

}
